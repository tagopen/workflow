{
  "name": "workflow",
  "version": "1.0.1",
  "description": "Website for tagopen.com",
  "main": "gulpfile.js",
  "author": "{%name%}, {%company%} <{%email%}>",
  "copyright": "tagopen",
  "license": "UNLICENSED",
  "private": true,
  "path": {
    "coFiles": "**/*.*",
    "src": {
      "base": "./app/",
      "html": "./app/",
      "js": "./app/js/",
      "css": "./app/css/",
      "img": "./app/img/",
      "sass": "./app/sass/",
      "pug": "./app/views/",
      "fonts": "./app/fonts/",
      "sprite": "./app/sass/utils/",
      "svgTemplate": "./app/sass/utils/_sprite-svg-template.scss",
      "htaccess": "./app/.htaccess",
      "mail": "./app/mail/",
      "sitemap": "./app/sitemap.php"
    },
    "dist": {
      "base": "./dist/",
      "img": "./dist/img/",
      "css": "./dist/css/",
      "js": "./dist/js/",
      "html": "./dist/",
      "fonts": "./dist/fonts/",
      "htaccess": "./dist/",
      "mail": "./dist/mail/"
    },
    "watch": {
      "html": "./app/*.html",
      "js": "./app/js/**/*.js",
      "sass": "./app/sass/**/*.+(scss|sass)",
      "css": "./app/css/**/*.css",
      "template": "./app/views/**/*",
      "pug": "./app/views/**/[^_]*.pug",
      "img": "./app/img/**/*.{png,jpg,jpeg,gif}",
      "svg": "./app/img/**/*.svg",
      "spritePng": "./app/img/icons/png/**/*.png",
      "spriteSvg": "./app/img/icons/svg/**/*.svg",
      "fonts": "./app/fonts/**/*.{woff,woff2}",
      "fontawesome": "./app/fonts/font-awesome/**/*",
      "mail": "./app/mail/**/*",
      "htaccess": "./app/.htaccess"
    },
    "build": {
      "base": "./build/",
      "css": "./build/css/",
      "fontello": "./build/fonts/fontello/",
      "fonts": "./build/fonts/",
      "js": "./build/js/",
      "html": "./build/html/",
      "img": "./build/img/"
    },
    "favicon": {
      "src": "./app/img/logo/favicon.*",
      "dest": "./dist/img/logo/"
    },
    "sass": []
  },
  "vars": {
    "siteCssName": "site.combined.min.css",
    "scssName": "style.scss",
    "cssName": "style.css",
    "jsName": "main.js"
  },
  "globs": {
    "babelJs": [
      "app/js/main.js"
    ],
    "inlineJs": [
      "//node_modules/jquery/dist/jquery.min.js"
    ],
    "distJs": [
      "node_modules/jquery/dist/jquery.min.js",
      "node_modules/wowjs/dist/wow.min.js",
      "node_modules/svg4everybody/dist/svg4everybody.min.js",
      "node_modules/jquery.maskedinput/src/jquery.maskedinput.js",
      "node_modules/parsleyjs/dist/parsley.js",
      "//node_modules/select2/dist/js/select2.full.min.js",
      "//node_modules/air-datepicker/dist/js/datepicker.min.js",
      "//node_modules/ion-rangeslider/js/ion.rangeSlider.min.js",
      "//node_modules/tooltipster/dist/js/tooltipster.bundle.min.js",
      "//node_modules/tooltipster/dist/js/SVG/tooltipster-SVG.min.js",
      "//node_modules/@fancyapps/fancybox/dist/jquery.fancybox.min.js",
      "//node_modules/vue/dist/vue.min.js",
      "//node_modules/vue-carousel-3d/dist/vue-carousel-3d.min.js",
      "//node_modules/jquery-countdown/dist/jquery.countdown.min.js",
      "//node_modules/owl.carousel/dist/owl.carousel.min.js",
      "./build/js/*.js"
    ]
  },
  "homepage": "https://github.com/tagopen/workflow#readme",
  "dependencies": {
    "@fancyapps/fancybox": "^3.3.5",
    "air-datepicker": "^2.2.3",
    "bootstrap": "^4.1.1",
    "bootstrap-validator": "^0.11.9",
    "fancybox": "^3.0.1",
    "fg-loadcss": "^2.0.1",
    "font-awesome": "^4.7.0",
    "fontfaceobserver": "^2.0.13",
    "ion-rangeslider": "^2.2.0",
    "jquery": "^3.2.1",
    "jquery-countdown": "^2.2.0",
    "jquery.cookie": "^1.4.1",
    "jquery.maskedinput": "^1.4.1",
    "lazysizes": "^4.0.2",
    "loadjs": "^3.5.4",
    "onepage-scroll": "^1.3.0",
    "owl.carousel": "^2.2.0",
    "parsleyjs": "^2.8.1",
    "select2": "^4.0.6-rc.1",
    "slick-carousel": "^1.8.1",
    "svg4everybody": "^2.1.9",
    "tiny-cookie": "^2.1.2",
    "tooltipster": "^4.2.6",
    "vue": "^2.5.13",
    "vue-carousel-3d": "^0.1.20",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bemto.pug": "^2.1.0",
    "browser-sync": "^2.24.4",
    "critical": "^1.3.0",
    "del": "^3.0.0",
    "fancy-log": "^1.3.2",
    "git-config": "0.0.7",
    "git-rev-sync": "^1.12.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-bower": "0.0.14",
    "gulp-cache": "^1.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-cheerio": "^0.6.2",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.5.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-data": "^1.2.1",
    "gulp-font2css": "^0.1.2",
    "gulp-html-minifier": "^0.1.8",
    "gulp-html-version": "^1.0.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-inject-string": "^1.1.0",
    "gulp-javascript-obfuscator": "^1.1.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.1.0",
    "gulp-plumber-notifier": "0.0.3",
    "gulp-prettier": "^2.0.0",
    "gulp-print": "^5.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-pxtorem": "^3.0.0",
    "gulp-rcs": "^1.0.1",
    "gulp-remember": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^4.0.1",
    "gulp-selectors": "^0.1.9",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-svg-sprite": "^1.4.0",
    "gulp-svgmin": "^1.2.3",
    "gulp-tinypng-nokey": "^1.1.0",
    "gulp-uglifyjs": "^0.6.2",
    "gulp-useref": "^3.1.5",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.0",
    "gulp.spritesmith": "^6.4.0",
    "vinyl-ftp": "^0.6.0"
  },
  "scripts": {
    "start": "gulp",
    "build": "gulp build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tagopen/workflow.git"
  },
  "bugs": {
    "email": "Artem2431@gmail.com"
  }
}
